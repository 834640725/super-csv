<!-- ====================================================================== -->
<!-- Ant build file (http://ant.apache.org/) for Ant 1.6.2 or above.        -->
<!--                                                                        -->
<!-- Super CSV uses Maven 3, but this Ant build file is provided to support -->
<!-- basic Ant builds for those who don't use Maven.                        -->
<!-- ant jar     - builds the jar                                           -->
<!-- ant javadoc - generates the javadoc                                    -->
<!-- ====================================================================== -->
<project name="super-csv-parent" default="package" basedir=".">

	<!-- ================================================================== -->
	<!-- Import maven-build.xml into the current project                    -->
	<!-- ================================================================== -->
	<import file="maven-build.xml"/>

	<!-- ================================================================== -->
	<!-- Help target                                                        -->
	<!-- ================================================================== -->
	<target name="help">
		<echo message="Please run: $ant -projecthelp"/>
	</target>

</project>
